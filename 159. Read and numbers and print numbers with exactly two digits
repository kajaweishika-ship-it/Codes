#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers with exactly 2 digits: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        if(num>=10 && num<=99 || num<=-10 && num>=-99) printf("%d ", num);
    }
    return 0;
}
