#include <stdio.h>
int main() {
    int ch,a,b;
    printf("1.Add 2.Sub 3.Mul: ");
    scanf("%d",&ch);
    printf("Enter two numbers: ");
    scanf("%d%d",&a,&b);
    switch(ch) {
        case 1: printf("%d",a+b); break;
        case 2: printf("%d",a-b); break;
        case 3: printf("%d",a*b); break;
        default: printf("Invalid");
    }
}
