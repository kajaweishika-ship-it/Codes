#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers with even sum of digits: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        int sum=0,temp=num;
        if(temp<0) temp=-temp;
        while(temp!=0){
            sum += temp%10;
            temp/=10;
        }
        if(sum%2==0) printf("%d ", num);
    }
    return 0;
}
