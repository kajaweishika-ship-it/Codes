#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i, flag=0;
    printf("Enter string: ");
    gets(s);
    for(i=0; s[i]!='\0'; i++)
        if(!(s[i]>='a'&&s[i]<='z') && !(s[i]>='A'&&s[i]<='Z') && !(s[i]>='0'&&s[i]<='9'))
            flag=1;
    if(flag) printf("Contains special character");
    else printf("No special characters");
}
