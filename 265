#include <stdio.h>
int main() {
    int units;
    printf("Enter units: ");
    scanf("%d",&units);
    switch(units/100) {
        case 0: printf("Bill = %d", units*2); break;
        case 1:
        case 2: printf("Bill = %d", units*3); break;
        default: printf("Bill = %d", units*5);
    }
}
