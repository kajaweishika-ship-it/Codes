#include <stdio.h>
int main() {
    int start = 2, end = 50, sum = 0, flag;
    for(int n = start; n <= end; n++) {
        flag = 0;
        for(int i = 2; i <= n/2; i++) {
            if(n % i == 0) { flag = 1; break; }
        }
        if(flag == 0) sum += n;
    }
    printf("Sum of primes between %d-%d = %d", start, end, sum);
    return 0;
}
