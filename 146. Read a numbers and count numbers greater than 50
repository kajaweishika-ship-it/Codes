#include <stdio.h>
int main() {
    int n, num, count=0;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Enter %d numbers: ", n);
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        if(num > 50) count++;
    }
    printf("Numbers greater than 50 = %d", count);
    return 0;
}
