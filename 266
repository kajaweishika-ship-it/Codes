#include <stdio.h>
int main() {
    int n,i,c=0,ch;
    printf("1.Check Prime 2.Exit: ");
    scanf("%d",&ch);
    switch(ch) {
        case 1: printf("Enter number: "); scanf("%d",&n);
                for(i=1;i<=n;i++) if(n%i==0) c++;
                switch(c==2){ case 1: printf("Prime"); break; case 0: printf("Not Prime");}
                break;
        case 2: printf("Exit");
    }
}
