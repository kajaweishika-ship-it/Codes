#include <stdio.h>
int main() {
    int n, num;
    printf("How many numbers? ");
    scanf("%d",&n);
    printf("Enter %d numbers: ", n);
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        if(num%2==0) printf("%d is even\n", num);
        else printf("%d is odd\n", num);
    }
    return 0;
}
