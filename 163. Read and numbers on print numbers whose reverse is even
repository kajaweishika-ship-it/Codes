#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers whose reverse is even: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        int rev=0,temp=num;
        if(temp<0) temp=-temp;
        while(temp!=0){
            rev = rev*10 + temp%10;
            temp/=10;
        }
        if(rev%2==0) printf("%d ", num);
    }
    return 0;
}
