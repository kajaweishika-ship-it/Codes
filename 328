#include <stdio.h>
#include <string.h>
int main() {
    char a[50], b[50];
    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);
    strlwr(a); strlwr(b);
    if(strlen(a)!=strlen(b))
        printf("Not anagrams");
    else {
        strrev(b);
        if(strcmp(a,b)==0)
            printf("Simple reverse anagram");
        else
            printf("Not simple reverse anagram");
    }
}
