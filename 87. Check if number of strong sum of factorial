#include <stdio.h>
int main() {
    int n = 145, temp = n, sum = 0, digit, fact, i;
    while(n != 0) {
        digit = n % 10;
        fact = 1;
        for(i = 1; i <= digit; i++) {
            fact *= i;
        }
        sum += fact;
        n /= 10;
    }
    if(sum == temp)
        printf("Strong Number");
    else
        printf("Not Strong Number");
    return 0;
}
