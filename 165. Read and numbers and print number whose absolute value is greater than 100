#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Numbers with absolute value > 100: ");
    for(int i=0; i<n; i++){
        scanf("%d", &num);
        if(num < 0) num = -num;
        if(num > 100) printf("%d ", num);
    }
    return 0;
}
