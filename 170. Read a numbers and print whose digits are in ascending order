#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers with digits in ascending order: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        int temp=num;
        if(temp<0) temp=-temp;
        int prev=10, flag=1;
        while(temp!=0){
            int digit = temp%10;
            if(digit > prev){ flag=0; break; }
            prev=digit;
            temp/=10;
        }
        if(flag) printf("%d ", num);
    }
    return 0;
}
