#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i, j, count=0;
    printf("Enter string: ");
    gets(s);
    for(i=0; i<strlen(s); i++)
        for(j=i+1; j<strlen(s); j++)
            if(s[i]==s[j] && s[i]!=' ') count++;
    printf("Repeated characters = %d", count);
}
