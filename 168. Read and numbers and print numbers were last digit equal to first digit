#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Numbers with first digit = last digit: ");
    for(int i=0; i<n; i++){
        scanf("%d", &num);
        int temp=num, last, first;
        if(temp<0) temp=-temp;
        last = temp%10;
        while(temp>=10) temp/=10;
        first = temp;
        if(first==last) printf("%d ", num);
    }
    return 0;
}
