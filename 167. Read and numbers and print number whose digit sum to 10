#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Numbers whose digits sum to 10: ");
    for(int i=0; i<n; i++){
        scanf("%d", &num);
        int temp=num, sum=0;
        if(temp<0) temp=-temp;
        while(temp!=0){
            sum += temp%10;
            temp/=10;
        }
        if(sum==10) printf("%d ", num);
    }
    return 0;
}
