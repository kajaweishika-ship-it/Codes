#include <stdio.h>
int main() {
    char ch;
    printf("Enter character: ");
    scanf("%c",&ch);
    switch(ch) {
        case 'a'...'z': case 'A'...'Z':
            printf("Alphabet"); break;
        case '0'...'9':
            printf("Digit"); break;
        default:
            printf("Special Character");
    }
}
