#include <stdio.h>
int main() {
    int marks;
    printf("Enter marks (0â€“100): ");
    scanf("%d",&marks);
    switch(marks/10) {
        case 10:
        case 9: printf("A+"); break;
        case 8: printf("A"); break;
        case 7: printf("B"); break;
        case 6: printf("C"); break;
        case 5: printf("D"); break;
        default: printf("Fail");
    }
}
