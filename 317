#include <stdio.h>
#include <string.h>
int main() {
    char s[100]; int i, flag=0;
    printf("Enter string: ");
    gets(s);
    for(i=0; s[i]!='\0'; i++) {
        if(!((s[i]>='a'&&s[i]<='z')||(s[i]>='A'&&s[i]<='Z'))) {
            flag=1; break;
        }
    }
    if(flag) printf("Not alphabetic");
    else printf("Alphabetic string");
}
