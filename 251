#include <stdio.h>
int main() {
    int n, i;
    printf("Enter number: ");
    scanf("%d",&n);
    switch(n > 0) {
        case 1:
            for(i=1;i<=10;i++)
                printf("%d x %d = %d\n",n,i,n*i);
            break;
        default:
            printf("Enter positive number");
    }
}
