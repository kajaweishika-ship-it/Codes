#include <stdio.h>
int main(){
    int n,ch,sum=0,i;
    printf("1.Odd Sum 2.Even Sum: ");
    scanf("%d",&ch);
    switch(ch){
        case 1: for(i=1;i<=10;i+=2) sum+=i; printf("Sum=%d",sum); break;
        case 2: for(i=2;i<=10;i+=2) sum+=i; printf("Sum=%d",sum); break;
        default: printf("Invalid");
    }
}
