#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    int count=0;
    printf("Enter a string: ");
    scanf(" %[^\n]", str);
    for(int i=0;i<strlen(str);i++){
        if(str[i]>='a' && str[i]<='z') count++;
    }
    printf("Number of lowercase letters = %d", count);
    return 0;
}
