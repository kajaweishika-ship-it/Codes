#include <stdio.h>
int main() {
    int count = 0, num = 2, i, flag;
    while(count < 10) {
        flag = 0;
        for(i = 2; i <= num/2; i++) {
            if(num % i == 0) {
                flag = 1; break;
            }
        }
        if(flag == 0) {
            printf("%d ", num);
            count++;
        }
        num++;
    }
    return 0;
}
