#include <stdio.h>
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n % 2 == 0 && n % 3 == 0)
        printf("Divisible by both (Not counted)\n");
    else if (n % 2 == 0 || n % 3 == 0)
        printf("Divisible by 2 or 3\n");
    else
        printf("Not Divisible\n");

    return 0;
}
