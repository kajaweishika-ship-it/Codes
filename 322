#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i, d=0, l=0, sp=0;
    printf("Enter password: ");
    gets(s);
    for(i=0; s[i]!='\0'; i++) {
        if(s[i]>='0'&&s[i]<='9') d=1;
        else if((s[i]>='a'&&s[i]<='z')||(s[i]>='A'&&s[i]<='Z')) l=1;
        else sp=1;
    }
    if(d && l && sp)
        printf("Strong password");
    else
        printf("Weak password");
}
