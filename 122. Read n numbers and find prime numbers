#include <stdio.h>
int main() {
    int n;
    printf("How many numbers? ");
    scanf("%d",&n);
    int num, flag;
    printf("Enter %d numbers: ", n);
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        flag = 0;
        for(int j=2;j<=num/2;j++){
            if(num%j==0) {flag=1; break;}
        }
        if(num>1 && flag==0) printf("%d is prime\n", num);
    }
    return 0;
}
