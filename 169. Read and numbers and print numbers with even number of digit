#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers with even number of digits: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        int temp=num, digits=0;
        if(temp<0) temp=-temp;
        while(temp!=0){
            temp/=10;
            digits++;
        }
        if(digits%2==0) printf("%d ", num);
    }
    return 0;
}
