#include <stdio.h>
void frequency(int a[],int n){
    int visited[100]={0};
    for(int i=0;i<n;i++){
        if(visited[i]) continue;
        int count=1;
        for(int j=i+1;j<n;j++)
            if(a[i]==a[j]){count++; visited[j]=1;}
        printf("%d -> %d times\n",a[i],count);
    }
}
int main(){
    int a[]={1,2,2,3,1,4,3};
    frequency(a,7);
}
