#include <stdio.h>
int main() {
    int m;
    printf("Enter month (1â€“12): ");
    scanf("%d", &m);
    switch((m-1)/3 + 1) {
        case 1: printf("Quarter 1"); break;
        case 2: printf("Quarter 2"); break;
        case 3: printf("Quarter 3"); break;
        case 4: printf("Quarter 4"); break;
        default: printf("Invalid");
    }
}
