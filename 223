#include <stdio.h>
int countGreaterAvg(int a[], int n){
    int sum=0;
    for(int i=0;i<n;i++) sum+=a[i];
    float avg=(float)sum/n;
    int c=0;
    for(int i=0;i<n;i++) if(a[i]>avg) c++;
    return c;
}
int main(){
    int a[]={10,20,30,40,50};
    printf("Greater than avg = %d", countGreaterAvg(a,5));
}
