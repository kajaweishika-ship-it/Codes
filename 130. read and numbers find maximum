#include <stdio.h>
int main() {
    int n, num, max;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    scanf("%d", &max); // first element as initial max
    for(int i=1; i<n; i++){
        scanf("%d", &num);
        if(num > max) max = num;
    }
    printf("Maximum = %d", max);
    return 0;
}
