#include <stdio.h>
int main(){
    int d;
    printf("Enter day number (1â€“7): ");
    scanf("%d",&d);
    switch(d){
        case 1: case 2: case 3: case 4: case 5:
            printf("Weekday"); break;
        case 6: case 7:
            printf("Weekend"); break;
        default: printf("Invalid");
    }
}
