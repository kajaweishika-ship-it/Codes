#include <stdio.h>
int main() {
    int n;
    printf("How many numbers? ");
    scanf("%d",&n);
    int arr[n], sum=0;
    printf("Enter %d numbers: ", n);
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        sum += arr[i];
    }
    printf("Average = %.2f", sum/(float)n);
    return 0;
}
