#include <stdio.h>
#include <string.h>
int main() {
    char pass[20], retype[20];
    printf("Enter password: ");
    gets(pass);
    printf("Re-enter password: ");
    gets(retype);
    if(strcmp(pass, retype)==0)
        printf("Password Matched");
    else
        printf("Password Mismatch");
}
