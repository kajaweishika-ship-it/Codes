#include <stdio.h>
int main() {
    int n, even=0, odd=0;
    printf("How many numbers? ");
    scanf("%d",&n);
    int num;
    printf("Enter %d numbers: ", n);
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        if(num%2==0) even++;
        else odd++;
    }
    printf("Even=%d, Odd=%d", even, odd);
    return 0;
}
