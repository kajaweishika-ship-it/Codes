#include <stdio.h>
int main() {
    int n, num;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    printf("Numbers with first digit odd: ");
    for(int i=0;i<n;i++){
        scanf("%d",&num);
        int temp=num;
        if(temp<0) temp=-temp;
        while(temp>=10) temp/=10;
        if(temp%2!=0) printf("%d ", num);
    }
    return 0;
}
